<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>
    <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/material_blue_grey_800</item>
        <item name="background">@color/material_blue_grey_800</item>
    </style>
    <style name="MyButton" parent="@style/Widget.AppCompat.Button">
        <item name="android:backgroundTint">#EF6C00</item>
        <item name="android:textColor">#ECEFF1</item>
    </style>
    <style name="MyCapstoneButton" parent="MyButton">
        <item name="android:backgroundTint">#E65100</item>
    </style>
    <style name="MyTitle" parent="@android:style/TextAppearance.Large">
        <item name="android:textColor">#607D8B</item>
    </style>
</resources>
